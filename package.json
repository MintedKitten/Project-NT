{
  "name": "nv-procurement-project",
  "version": "0.9.1",
  "private": true,
  "type": "module",
  "scripts": {
    "install": "npm list",
    "prod": "npm run build && npm run start",
    "dev": "npm run prerun && npm run dev:real",
    "prerun_build_start": "npm run prerun && npm run build && npm run start",
    "build": "npm run build:real",
    "start": "npm run start:real",
    "lint": "npm run prerun && npm run lint:real",
    "prerun": "npm run prerun:node && npm run prerun:server",
    "prerun:node": "esbuild next.config.ts --bundle --format=esm --outfile=next.config.mjs --platform=node --external:./node_modules/* --target=es2020 --minify --allow-overwrite",
    "prerun:server": "esbuild start.ts --bundle --format=esm --outfile=start.mjs --platform=node --external:./node_modules/* --target=es2020 --minify --allow-overwrite",
    "dev:real": "cross-env NODE_ENV=development node -r dotenv/config start.mjs dotenv_config_path=.env.local",
    "build:real": "next build",
    "start:real": "cross-env NODE_ENV=production node -r dotenv/config start.mjs dotenv_config_path=.env.local",
    "lint:real": "next lint"
  },
  "dependencies": {
    "@date-io/core": "^2.15.0",
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/lab": "^5.0.0-alpha.97",
    "@mui/material": "^5.10.3",
    "@mui/x-data-grid": "^5.17.1",
    "@mui/x-date-pickers": "^5.0.0",
    "big.js": "^6.2.1",
    "bson": "^4.7.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "filesize": "^9.0.11",
    "formidable": "^2.0.1",
    "mongodb": "^4.9.1",
    "next": "^12.2.5",
    "next-auth": "^4.10.3",
    "next-connect": "^0.13.0",
    "nextjs-progressbar": "^0.0.14",
    "papaparse": "^5.3.2",
    "react": "^18.2.0",
    "react-detect-offline": "^2.4.5",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-file-icon": "^1.2.0",
    "react-mui-confirm": "^1.0.1",
    "seedrandom": "^3.0.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/big.js": "^6.1.5",
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.5",
    "@types/node": "^18.7.15",
    "@types/papaparse": "^5.3.5",
    "@types/react": "^18.0.18",
    "@types/react-detect-offline": "^2.4.1",
    "@types/react-file-icon": "^1.0.1",
    "@types/seedrandom": "^3.0.2",
    "esbuild": "^0.15.7",
    "eslint": "^8.23.0",
    "eslint-config-next": "^12.2.5",
    "typescript": "^4.8.3"
  }
}
