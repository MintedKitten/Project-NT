{
  "name": "nv-procurement-project",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run prerun && npm run dev:real",
    "build": "npm run prerun && npm run build:real",
    "start": "npm run start:real",
    "lint": "npm run prerun && npm run lint:real",
    "prerun": "esbuild next.config.ts --bundle --format=esm --outfile=next.config.mjs --platform=node --external:./node_modules/* --target=es2020 --minify --allow-overwrite",
    "prerun:dev": "esbuild start.ts --bundle --format=esm --outfile=start.mjs --platform=node --external:./node_modules/* --target=es2020 --allow-overwrite",
    "prerun:start": "esbuild start.ts --bundle --outfile=start.js --platform=node --external:./node_modules/* --target=es2020 --allow-overwrite",
    "dev:real": "cross-env NODE_ENV=development nodemon start.mjs",
    "build:real": "next build",
    "start:real": "cross-env NODE_ENV=production node start.js",
    "lint:real": "next lint"
  },
  "dependencies": {
    "@date-io/core": "^2.15.0",
    "@date-io/dayjs": "^2.15.0",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@mui/x-date-pickers": "^5.0.0-beta.6",
    "@types/big.js": "^6.1.5",
    "big.js": "^6.2.1",
    "bson": "^4.7.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "esbuild": "^0.15.5",
    "express": "^4.18.1",
    "filesize": "^9.0.11",
    "formidable": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.9.0",
    "next": "latest",
    "next-auth": "^4.10.3",
    "next-connect": "^0.13.0",
    "nextjs-progressbar": "^0.0.14",
    "nodemon": "^2.0.19",
    "papaparse": "^5.3.2",
    "react": "latest",
    "react-device-detect": "^2.2.2",
    "react-dom": "latest",
    "react-mui-confirm": "^1.0.1",
    "seedrandom": "^3.0.5",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "latest",
    "@types/papaparse": "^5.3.3",
    "@types/react": "latest",
    "@types/seedrandom": "^3.0.2",
    "babel-plugin-import": "^1.13.5",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "typescript": "latest"
  }
}
